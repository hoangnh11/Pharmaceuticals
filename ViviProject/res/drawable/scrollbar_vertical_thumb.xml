<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item
        android:bottom="8dp"
        android:top="8dp"
        android:left="8dp"
        android:right="8dp">
        <shape android:shape="rectangle" >
            <size
                android:height="45dp"
                android:width="5dp" />

            <solid android:color="#33B5E5" />

            <padding
                android:left="10dp"
                android:right="10dp" />
            
            <corners android:radius="6dp"/>
        </shape>
    </item>

</layer-list>